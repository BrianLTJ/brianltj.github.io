<%- partial('partial/headerXBig',{showHeaderContent: true,showHeaderContentPlaceholder:false}) %>
<main>
    <div class="container">
        <div class="row">
            <!-- Post Feed -->
            <div class="col s12 xl9 post-feed">
                <% site.posts.forEach(function(post){ %>
                    <%- partial('partial/postListItemNormal', {post:post}) %>
                <%})%>

                <!-- Page Switcher -->
                <div class="row">
                <!-- Last Page -->
                <a class="waves-effect waves-teal btn-flat" href="#!"><i class="material-icons">chevron_left</i></a>
                <!-- Next Page -->
                <a class="waves-effect waves-teal btn-flat" href="#!"><i class="material-icons">chevron_right</i></a>
            </div>
            <!-- End Page Switcher -->
            
            </div>
            <!-- End Post Feed -->

            <!-- Side Bar Area -->
        <div class="col s12 xl3 side-widgets">

          <div class="card" id="side-category">
            <div class="card-content">
              <span class="card-title">CATEGORIES</span>
              <ul class="category-list cate-tag-list">
                <li class="category-item cate-tag-item"><a href="#">Def Cate1</a></li>
                <li class="category-item cate-tag-item"><a href="#">Def Cate1</a></li>
                <li class="category-item cate-tag-item"><a href="#">Def Cate1</a></li>
                <li class="category-item cate-tag-item"><a href="#">Def Cate1</a></li>
              </ul>
            </div>
          </div>
          
          <div class="card">
            <div class="card-content">
              <span class="card-title">TAGS</span>
              <ul class="tag-list cate-tag-list">
                  <li class="tag-item cate-tag-item"><a href="#">Def Cate1</a></li>
                  <li class="tag-item cate-tag-item"><a href="#">Def Cate1</a></li>
                  <li class="tag-item cate-tag-item"><a href="#">Def Cate1</a></li>
                  <li class="tag-item cate-tag-item"><a href="#">Def Cate1</a></li>
                </ul>
            </div>
          </div>

      </div>


        </div>
        <!-- End row -->
    </div>
    <!-- End Container -->
</main>